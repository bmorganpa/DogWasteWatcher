{
  "extends": ["eslint:recommended", "react-app", "prettier"],
  "env": {
    "browser": true,
    "commonjs": true,
    "jest": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn"
  }
}
